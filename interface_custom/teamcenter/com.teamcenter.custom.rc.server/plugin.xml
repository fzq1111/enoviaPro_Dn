<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Start Category"
            id="com.teamcenter.custom.rc.server.commands.startCategory">
      </category>
      <command
            name="Start"
            categoryId="com.teamcenter.custom.rc.server.commands.startCategory"
            id="com.teamcenter.custom.rc.server.commands.startCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.teamcenter.custom.rc.server.commands.startCommand"
            class="com.teamcenter.custom.rc.server.handlers.ServiceHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.teamcenter.custom.rc.server.commands.startCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Service"
               mnemonic="S"
               id="com.teamcenter.custom.rc.server.menus.serviceMenu">
            <command
                  commandId="com.teamcenter.custom.rc.server.commands.startCommand"
                  mnemonic="S"
                  id="com.teamcenter.custom.rc.server.menus.startCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.teamcenter.custom.rc.server.toolbars.serviceToolbar">
            <command
                  commandId="com.teamcenter.custom.rc.server.commands.startCommand"
                  icon="icons/sample.gif"
                  tooltip="Start service"
                  id="com.teamcenter.custom.rc.server.toolbars.startCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
