	/*
	 * @author Yun.Lee 2016/12/15 (non-Javadoc)
	 * 
	 * @see ${CLASS:emxAEFFullSearchBase}#search(matrix.db.Context,
	 * java.lang.String[])
	 */
	@Override
	public MapList search(Context context, String[] args) throws Exception {
		Map params = null;
		String dataSource = DomainObject.EMPTY_STRING;
		try {
			params = JPO.unpackArgs(args);
			dataSource = ((String) params.get("dataSource"));
		} catch (Exception ex) {
			ex.printStackTrace();
			throw ex;
		}
		if ("teamcenter".equalsIgnoreCase(dataSource)) {
			return JPO.invoke(context, "emxTeamcenterClient", null, "search", args, MapList.class);
		}
		// TODO search something? copy super.search code here and change it.

		return super.search(context, args);
	}

	/**
	 * @param args
	 */
